package flash.net {

/**
 * A simple encapsulation of a URL.
 */
public class URLRequest
{
    /**
     * The URL to be requested.
     */
    public var url :String;

    /**
     * The URL to be requested.
     */
    public function URLRequest (url: String = null)
    {
        this.url = url;
    }
}
}
